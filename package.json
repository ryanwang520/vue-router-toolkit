{
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "sideEffects": false,
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "scripts"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "postinstall": "node ./scripts/postinstall.js",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {
    "dayjs": "^1.10.4",
    "vue-demi": "^0.4.5"
  },
  "devDependencies": {
    "tsdx": "^0.14.1",
    "vue": "^2.6.0",
    "vue-router": "^3.5.1",
    "@vue/composition-api": "^1.0.0-rc.2"
  },
  "peerDependencies": {
    "vue": "^2.6.0 || >=3.0.0",
    "vue-router": "^3.5.1 || >= 4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "vue-router-toolkit",
  "author": "Ryan Wang",
  "module": "dist/vuer-router-toolkit.esm.js",
  "size-limit": [
    {
      "path": "dist/vuer-router-toolkit.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/vuer-router-toolkit.esm.js",
      "limit": "10 KB"
    }
  ]
}
